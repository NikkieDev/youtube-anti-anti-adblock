{
	"manifest_version": 3,
	"name": "Youtube Adblock Helper",
	"description": "Blocks the youtube adblocker blocker",
	"permissions": ["storage"],
	"author": "NikkieDev",
	"version": "1.4",
	"content_scripts": [
		{
			"matches": ["https://youtube.com/*", "https://www.youtube.com/*"],
			"js": ["main.js"]
		}
	],
	"action": {
		"default_popup": "ui/data.html",
		"default_icon": "resources/icon.png"
	}
}
